<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>

    <groupId>com.devyy</groupId>
    <artifactId>openyspider</artifactId>
    <version>3.0-SNAPSHOT</version>
    <packaging>pom</packaging>

    <modules>
        <module>oys-core</module>
        <module>oys-yande</module>
        <module>oys-meinvla</module>
        <module>oys-tujidao</module>
        <module>oys-boot-starter</module>
        <module>oys-leetcode</module>
    </modules>

    <properties>
        <!-- 三方库 -->
        <springboot_version>2.2.4.RELEASE</springboot_version>
        <springcloud_version>Greenwich.SR1</springcloud_version>
        <lombok_version>1.18.6</lombok_version>
        <mybatisplus_version>3.3.0</mybatisplus_version>
        <swagger_version>2.9.2</swagger_version>
        <mysql_connecter_version>8.0.18</mysql_connecter_version>
        <ojdbc_version>12.1.0.2</ojdbc_version>

        <jsoup_version>1.12.1</jsoup_version>
        <selenium_version>3.14.0</selenium_version>
        <fastjson_version>1.2.66</fastjson_version>

        <!-- 一方库 -->
        <revision>3.0-SNAPSHOT</revision>
    </properties>

    <dependencyManagement>
        <dependencies>
            <dependency>
                <groupId>org.springframework.boot</groupId>
                <artifactId>spring-boot-dependencies</artifactId>
                <version>${springboot_version}</version>
                <type>pom</type>
                <scope>import</scope>
            </dependency>
            <dependency>
                <groupId>org.springframework.cloud</groupId>
                <artifactId>spring-cloud-dependencies</artifactId>
                <version>${springcloud_version}</version>
                <type>pom</type>
                <scope>import</scope>
            </dependency>
        </dependencies>
    </dependencyManagement>

    <build>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-compiler-plugin</artifactId>
                <configuration>
                    <source>8</source>
                    <target>8</target>
                </configuration>
            </plugin>
        </plugins>
    </build>

</project>